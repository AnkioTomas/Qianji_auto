<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Qianji_auto" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
       

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="lollipop">#00FFFFFF</item><!--设置状态栏的颜色-->
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <!-- Customize your theme here. -->
        <!--无Title-->
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorBlueAlpha20</item>
        <item name="colorPrimaryVariant">@color/colorBlueAlpha20</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorBlueAlpha50</item>
        <item name="colorSecondaryVariant">@color/colorBlueAlpha50</item>
        <item name="colorOnSecondary">@color/white</item>

        <item name="cookieTitleColor">@color/default_title_color</item>
        <item name="cookieMessageColor">@color/default_message_color</item>
        <item name="cookieActionColor">@color/default_title_color</item>
        <item name="cookieBackgroundColor">@color/button_go_setting_bg</item>
    </style>
    
    


</resources>